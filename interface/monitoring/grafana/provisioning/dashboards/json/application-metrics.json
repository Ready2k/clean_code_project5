{
  "dashboard": {
    "id": null,
    "title": "Prompt Library - Application Metrics",
    "tags": ["prompt-library", "application"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Prompt Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(prompt_operations_total[5m])",
            "legendFormat": "{{operation}} - {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Enhancement Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(enhancement_operations_total[5m])",
            "legendFormat": "{{provider}} - {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Enhancement Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(enhancement_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(enhancement_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Authentication Attempts",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(authentication_attempts_total[5m])",
            "legendFormat": "{{status}} - {{method}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Cache Hit Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "cache_hit_ratio",
            "legendFormat": "Hit Ratio"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "WebSocket Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "websocket_connections_active",
            "legendFormat": "Active WebSocket Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 16}
      },
      {
        "id": 7,
        "title": "Top API Endpoints by Request Count",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (route) (rate(http_requests_total[5m])))",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Slowest API Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum by (route) (rate(http_request_duration_seconds_bucket[5m]))))",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}